[Script]
Call Plugin.File.DeleteFile("zb.txt")
Global 变量15, 弹窗文本,弹窗文本1, x, y, 当前路径, 输入框文本, 输入框文本1
弹窗文本 = InputBox("比例尺：")
弹窗文本1 = InputBox("比例尺1：")
Form1.InputBox1.Text = 弹窗文本
Form1.InputBox2.Text = 弹窗文本1
输入框文本 = Form1.InputBox1.Text
输入框文本1 = Form1.InputBox2.Text
当前路径 = Plugin.Sys.GetDir(0)
//宽
x = Plugin.GetSysInfo.GetScreenResolutionX()
//高
y = Plugin.GetSysInfo.GetScreenResolutionY()
本行内容 = Plugin.File.GetFileLength("zb.txt")
坐标文件 = Plugin.File.OpenFile("zb.txt")
Call Plugin.File.SeekFile(坐标文件, 本行内容)
Call Plugin.File.WriteLine(坐标文件, "" & 弹窗文本 & ""& Chr(10) &"" & 弹窗文本1 & "")
TracePrint "写入""" & intX & "," & intY & ""
Call Plugin.File.CloseFile(坐标文件) 
//下面这句是向目标文件("zb.txt")读出文本内容到变量(Text)
Text = Plugin.File.ReadFileEx("zb.txt")
TracePrint "向目标文件读出文本内容为："& Text
dim MyArray
//回车换行符被处理成管道符 | 显示
MyArray = Split(Text, "|")
If UBound(MyArray)>=0 Then
    i=0
    For UBound(MyArray)
        //下面这句是得到文本内容
        TracePrint "第 " & i+1 &" 行文本内容为："& Cstr(MyArray(i))
        i=i+1
    Next
End If
